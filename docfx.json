{
  "metadata": [
    {
      "src": [
        {
          "src": "../OAIAPI",
          "files": [
            "OAIAPI/*.csproj"
          ],
          "exclude": [
            "OAIAPI/Capabilities/Assistants/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "docs",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "OAIAPI",
      "_appTitle": "OAIAPI",
      "_enableSearch": true,
      "pdf": true
    }
  }
}